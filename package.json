{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "spelling": "cspell 'src/**/*.{js,jsx,md,ts,tsx}' --no-must-find-files -c cspell.json",
    "spelling:ci": "cspell run --no-must-find-files",
    "eslint:ci": "eslint --color --cache -f table",
    "eslint": "eslint src --color --cache -f table",
    "eslint:js:fix": "eslint src --fix",
    "prettier": "prettier --check src --config ./.prettierrc.cjs",
    "prettier:fix": "prettier --write src --config ./.prettierrc.cjs",
    "prettier:ci": "prettier --check --config ./.prettierrc.cjs",
    "check": "npm run prettier && npm run eslint",
    "lefthook": "lefthook run lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.2",
    "@tanstack/react-query": "^5.14.2",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "vite": "^5.1.4",
    "yup": "^1.3.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "16.2.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.14.0",
    "commitlint": "16.2.4",
    "cspell": "^5.19.7",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-formatter-table": "7.32.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "lefthook": "1.1.1",
    "markdown-link-check": "^3.10.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "typescript": "*"
  }
}
